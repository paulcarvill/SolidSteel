<p id="notice"><%= notice %></p>

<h1>Listing Tracks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tracks.each do |track| %>
      <tr>
        <td><%= link_to "#{track.artist} /  #{track.title}", track unless track.artist.nil? %></td>
      </tr>
    <% end %>
  </tbody>
</table>